@model IEnumerable<donorclub.Models.UserLevelModel>

@{
    ViewBag.Title = "Direct_team";
    Layout = "~/Views/Shared/User_Layout.cshtml";
}

<!--#INCLUDE file="head.asp"-->
<!--start page wrapper -->
<div class="page-wrapper">
    <div class="page-content">
        <!--breadcrumb-->
        <div class="page-breadcrumb d-none d-sm-flex align-items-center mb-3">
            <div class="ps-3">
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb mb-0 p-0">
                        <li class="breadcrumb-item">
                            <a href="javascript:;"><i class="bx bx-home-alt"></i> Dashboard</a>
                        </li>
                        <li class="breadcrumb-item active" aria-current="page">Income</li>
                    </ol>
                </nav>
            </div>
        </div>
        <!--end breadcrumb-->
        <h6 class="mb-0 text-uppercase">Direct Income</h6>
        <hr />
        <div class="card bg-dark">
            <div class="card-body">
                <div class="table-responsive">
                    <table id="example" class="table table-striped table-bordered" style="width:100%">
                        <thead>
                            <tr>
                                <th>Sr.No</th>
                                <th>joining Date</th>
                                <th>User ID</th>
                                <th>User Name</th>
                                <th>Email ID</th>
                                <th>Mobile Number</th>
                                
                            </tr>
                        </thead>
                        <tbody>
                            @{ int rows = 0;}
                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>@(rows = rows + 1)</td>
                                    <td>@item.reg_date</td>

                                    <td>@item.username</td>
                                    <td>@item.memb_name</td>

                                    @*<td>BUSD @item.Total_Business</td>
                    <td>@item.credit_date</td>*@
                                    <td>@item.email</td>
                                    <td>@item.Mobile_No</td>
                                    

                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>


    </div>
</div>
<!--end page wrapper -->
<!--start overlay-->
<div class="overlay toggle-icon"></div>
<!--end overlay-->
<!--Start Back To Top Button--> <a href="javaScript:;" class="back-to-top"><i class='bx bxs-up-arrow-alt'></i></a>
<!--End Back To Top Button-->
