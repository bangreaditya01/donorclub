@model donorclub.Models.PinTransferModel
@using LudoFoundation_app.CommanFunction;
@{

    UserInfo userInfo = Common.CurrentUserInfo;

    ViewBag.Title = "Pin_receivedHistory";
    Layout = "~/Views/Shared/User_Layout.cshtml";
}

<!--#INCLUDE file="head.asp"-->
<!--start page wrapper -->
<div class="page-wrapper">
    <div class="page-content">
        <!--breadcrumb-->
        <div class="page-breadcrumb d-none d-sm-flex align-items-center mb-3">
            <div class="ps-3">
                <nav aria-label="breadcrumb">
                    <ol class="breadcrumb mb-0 p-0">
                        <li class="breadcrumb-item">
                            <a href="javascript:;"><i class="bx bx-home-alt"></i> Dashboard</a>
                        </li>
                        <li class="breadcrumb-item active" aria-current="page">My Team</li>
                    </ol>
                </nav>
            </div>
        </div>
        <!--end breadcrumb-->
        <h6 class="mb-0 text-uppercase">Pin Received History</h6>
        <hr />
        <div class="card bg-dark">
            <div class="card-body">
                <div class="table-responsive">
                    <table id="example" class="table table-striped table-bordered" style="width:100%">
                        <thead>
                            <tr>
                                <th>Sr.No.</th>
                                <th>Date</th>
                                <th>User ID</th>
                                <th>User Name</th>
                                <th>Pin</th>
                                <th>Amount</th>

                                <th>  Status</th>
                            </tr>
                        </thead>
                        <tbody>
                            @{ int rows = 0;}
                            @foreach (var item in Model.receiveHistory)
                            {
                                <tr role="row" class="odd">
                                    <td>@(rows = rows + 1)</td>
                                    <td>@item.TF_Date</td>
                                    <td>@item.username</td>
                                    <td>@item.Memb_Name</td>
                                    <td>@item.Pin</td>
                                    <td>₹ @item.amount</td>
                                    <td>@item.u_flag</td>
                                </tr>
                            }
                        </tbody>

                    </table>
                </div>
            </div>
        </div>


    </div>
</div>
<!--end page wrapper -->
<!--start overlay-->
<div class="overlay toggle-icon"></div>
<!--end overlay-->
<!--Start Back To Top Button--> <a href="javaScript:;" class="back-to-top"><i class='bx bxs-up-arrow-alt'></i></a>
<!--End Back To Top Button-->
